    @*信息方面调研*@
    <div class="module-div">
        <h4>信息方面调研</h4>
        <table class="datalist" style="word-break:break-all;word-wrap:break-word;margin-top:10px;">
            <tbody>
                <tr>
                    <td class="table_title">调研人：</td>
                    <td class="td-div">
                        <div>@Model.TzResearchOfInformation.ReaearchUserName</div>
                    </td>
                    <td class="table_title">职务：</td>
                    <td class="td-div">
                        <div>@Model.TzResearchOfInformation.JobName</div>
                    </td>
                </tr>
                <tr>
                    <td class="table_title">改进措施：</td>
                    <td class="td-div" colspan="3" style="height:100px;">
                        <div>@Model.TzResearchOfInformation.Improvement</div>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>

    @*附件信息*@
    <div class="module-div">
        <h4>附件信息</h4>
        <table class="datalist" style="word-break:break-all;word-wrap:break-word;margin-top:10px;">
            <tbody>
                <tr>
                    <th>序号</th>
                    <th>附件名称</th>
                    <th>附件类型</th>
                    <th>操作</th>
                </tr>
                @if (Model.TzResearchOfInformation.TzAttachs != null && Model.TzResearchOfInformation.TzAttachs.Count() > 0)
                {
                    foreach (var item in Model.TzResearchOfInformation.TzAttachs)
                    {

                        <tr id="${obj.GuidId}">
                            <td><span class="font-black">@item.Sort</span></td>
                            <td class="text_lf td-div"><span class="font-black">@item.Name</span></td>
                            <td><span class="font-black">@item.TypeName</span></td>
                            <td class="td-div" style="text-align:center">
                                <a class="fileDel downloadFile" href="javascript:void(0)" data-id="@item.Id" data-url="@item.FilePath" data-name="@item.Name" data-typeno="@item.TypeNo" data-typename="@item.TypeName" style="color:#337ab7;">下载</a>
                            </td>
                        </tr>
                    }
                }
                else
                {
                    <tr>
                        <td colspan="4">暂无附件</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
<script>
    //证照类型
    var LicenseName="";
    var License = $("#License").val().split(",");
    for (var i = 0; i < License.length; i++) {

        if (License.indexOf(License[i]) != -1) {
            if (License[i] == "ZZLX1") {
                LicenseName = LicenseName + "工程规划" + '、';
            }
            else if (License[i] == "ZZLX2") {
                LicenseName = LicenseName + "用地规划" + '、';
            }
            else if (License[i] == "ZZLX3") {
                LicenseName = LicenseName + "土地证" + '、';
            }
            else if (License[i] == "ZZLX4") {
                LicenseName = LicenseName + "划危证" + '、';
            }
            else if (License[i] == "ZZLX5") {
                LicenseName = LicenseName + "消防验收" + '、';
            }
            else if (License[i] == "ZZLX6") {
                LicenseName = LicenseName + "营业执照" + '、';
            }
            else if (License[i] == "ZZLX7") {
                LicenseName = LicenseName + "成品油证" + '、';
            }
            else if (License[i] == "ZZLX8") {
                LicenseName = LicenseName + "房产证" + '、';
            }
        }
    }
    var str = LicenseName.substring(0, LicenseName.length - 1);
    $(".License").text(str);

</script>