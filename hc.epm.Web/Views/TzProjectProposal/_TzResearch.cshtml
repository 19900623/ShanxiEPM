                    </td>
                </tr>
                <tr>
                    <td class="table_title">特殊销售手段：</td>
                    <td class="td-div">
                        <div>
                            @if (Model.TzResearchOfManagement.SalesMeans != null)
                            {
                                if (Model.TzResearchOfManagement.SalesMeans == "TSSD1")
                                {
                                    <span>有</span>
                                }
                                else
                                {
                                    <span>无</span>
                                }
                            }
                        
                        </div>
                    </td>
                    <td class="table_title">销量可实现性：</td>
                    <td class="td-div">
                        <div>
                            @if (Model.TzResearchOfManagement.SalesRealizability != null)
                            {
                                if (Model.TzResearchOfManagement.SalesRealizability == "XIAOLKSX1")
                                {
                                    <span>是</span>
                                }
                                else
                                {
                                    <span>否 </span >
                                }
                            }

                        </div>
                    </td>
                </tr>
                <tr>
                    <td class="table_title">油品来源：</td>
                    <td class="td-div" colspan="3" style="height:100px;">
                        <div>@Model.TzResearchOfManagement.SourceOfOil</div>
                    </td>
                </tr>
            </tbody>
