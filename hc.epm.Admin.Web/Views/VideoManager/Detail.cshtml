@using hc.epm.DataModel.Basic;
@model Base_VideoManage
@{
    ViewBag.Title = "视频监控详情";
}
<div class="container">
    <div class="row">
        <form class="form-horizontal " id="mainForm">
            <div class="col-lg-12 col-sm-12" style="height:60px">
                <div class="form-group">
                    <label class="col-lg-2 col-sm-2 control-label"><em class="required"></em>项目ID : </label>
                    <div class="col-lg-6 col-sm-6">
                        <input type="text" class="form-control" value="@Model.ProjectName" readonly/>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-sm-12">
                <div class="form-group">
                    <label class="col-lg-2 col-sm-2 control-label"><em class="required"></em>URL地址：</label>
                    <div class="col-lg-6 col-sm-6">
                        <input type="text" class="form-control" value="@Model.UrlAddress" readonly/>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-sm-12">
                <div class="form-group">
                    <label class="col-lg-2 col-sm-2 control-label"><em class="required"></em>摄像头名称：</label>
                    <div class="col-lg-6 col-sm-6">
                        <input type="text" class="form-control" value="@Model.CameraName" readonly/>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-sm-12">
                <div class="form-group">
                    <label class="col-lg-2 col-sm-2 control-label"><em class="required"></em>施工方单位：</label>
                    <div class="col-lg-6 col-sm-6">
                        <input type="text" class="form-control" value="@Model.Companyname" readonly/>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-sm-12">
                <div class="form-group">
                    <label class="col-lg-2 col-sm-2 control-label"><em class="required"></em>施工姓名：</label>
                    <div class="col-lg-6 col-sm-6">
                        <input type="text" class="form-control" value="@Model.CompanyPerson" readonly/>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-sm-12">
                <div class="form-group">
                    <label class="col-lg-2 col-sm-2 control-label"><em class="required"></em>设备序列号：</label>
                    <div class="col-lg-6 col-sm-6">
                        <input type="text" class="form-control" value="@Model.VerificationCode" readonly/>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-sm-12">
                <div class="form-group">
                    <label class="col-lg-2 col-sm-2 control-label"><em class="required"></em>设备验证码：</label>
                    <div class="col-lg-6 col-sm-6">
                        <input type="text" class="form-control" value="@Model.DeviceSequence" readonly/>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-sm-12">
                <div class="form-group">
                    <label class="col-lg-2 col-sm-2 control-label"><em class="required"></em>设备描述：</label>
                    <div class="col-lg-8 col-sm-8">
                        <textarea class="form-control" rows="4" readonly>@Model.DescribeInfo</textarea>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>



